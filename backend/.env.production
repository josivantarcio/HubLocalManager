DB_SYNCHRONIZE=false
DB_SSL=true

# Application configuration
NODE_ENV=production
PORT=${PORT}
JWT_SECRET=${JWT_SECRET}
JWT_EXPIRATION=1d

# CORS configuration
CORS_ORIGIN=${CORS_ORIGIN}